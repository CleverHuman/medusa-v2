<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.medusa.mall.mapper.AkSkMapper">
    <resultMap id="AkSkResult" type="MallAkSk">
        <id     property="id"       column="id"      />
        <result property="accessKey"       column="access_key"       />
        <result property="secretKey"       column="secret_key"       />
        <result property="status" column="status"/>
    </resultMap>


    <select id="selectByAccessKey" parameterType="String" resultMap="AkSkResult">
        select * from mall_ak_sk where access_key = #{accessKey}
    </select>

</mapper>